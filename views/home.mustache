<!DOCTYPE html>
<html lang="en">
{{>head}}
<body>
	
    {{>header}}

    <!-- banner -->
    <div class="banner" id="hero_section">
        <div class="container">
            <div class="row banner-text">
                <div class="slider-info col-lg-6">
                    <div class="banner-info-grid mt-lg-5">
                        <h2>{{page.hero_section.title}}</h2>
                        <p>{{page.hero_section.description}}</p>
                    </div>
                    <a class="btn mr-2 text-capitalize" href="#register">{{page.hero_section.btn_text}}</a>
                </div>
                <div class="col-lg-6 col-md-8 mt-lg-0 mt-sm-5 mt-3 banner-image text-lg-center">
                    <img src="{{{page.hero_section.img_hero}}}" alt="" class="img-fluid"/>
                </div>
            </div>
        </div>
    </div>
    <!-- //banner -->

    <!-- about -->
    {{#page.section1.show_or_hide}}
    <section class="about py-5" id="section1">
        <div class="container py-lg-5 py-sm-3">
            <div class="row">
                <div class="col-lg-3 about-left">	
                    <h3 class="heading mb-lg-5 mb-4">{{page.section1.title}}</h3>
                </div>
                <div class="col-lg-5 col-md-7 about-text">
                    <h3>{{page.section1.subtitle}}</h3>
                    <p class="mt-3">{{page.section1.description}}</p>
                    <p class="mt-2">{{page.section1.description2}}</p>
                </div> 
                <div class="col-lg-4 col-md-5 about-img">
                    <img src="{{{page.section1.img_section}}}" alt="" class="img-fluid"/>
                </div>
            </div>
        </div>
    </section>
    {{/page.section1.show_or_hide}}
    <!-- //about -->

    <!-- why choose us -->
    {{#page.section2.show_or_hide}}
    <section class="choose py-5" id="section2">
        <div class="container py-md-3">
            <h3 class="heading mb-5">{{page.section2.title}}</h3>
            <div class="feature-grids row">
                {{#services}}
                <div class="col-lg-3 col-sm-6">
                    <div class="f1 icon1 p-4">
                        <div class="icon">
                            <span class="fa"><img src="{{{image}}}"/></span>
                        </div>
                        <h3 class="my-3">{{name}}</h3>
                        <p>{{description}}</p>
                    </div>
                </div>
                {{/services}}
            </div>
        </div>
    </section>
    {{/page.section2.show_or_hide}}
    <!-- //why choose us -->

    <!-- Offered Services -->
    {{#page.section3.show_or_hide}}
    <section class="process py-5" id="section3">
        <div class="container py-md-3">
            <h3 class="heading mb-5">Marketing Process</h3>
            <div class="row process-grids">
                <div class="col-lg-3 col-md-6 my-lg-4 w3pvt-ab position-relative">
                    <div class="">
                        <img src="{{{page.section3.lists.list1.img_list}}}" alt="" class="img-fluid">
                    </div>
                    <h4 class="feed-title my-md-3 mb-3">{{page.section3.lists.list1.title}}</h4>
                    <p>{{page.section3.lists.list1.description}}</p>
                </div>
                <div class="col-lg-3 col-md-6 my-lg-4 mt-md-0 mt-4 w3pvt-ab position-relative">
                    <div class="">
                        <img src="{{{page.section3.lists.list2.img_list}}}" alt="" class="img-fluid">
                    </div>
                    <h4 class="feed-title my-md-3 mb-3">{{page.section3.lists.list2.title}}</h4>
                    <p>{{page.section3.lists.list2.description}}</p>
                </div>
                <div class="col-lg-3 col-md-6 my-lg-4 mt-sm-5 mt-4 w3pvt-ab position-relative">
                    <div class="">
                        <img src="{{{page.section3.lists.list3.img_list}}}" alt="" class="img-fluid">
                    </div>
                    <h4 class="feed-title my-md-3 mb-3">{{page.section3.lists.list3.title}}</h4>
                    <p>{{page.section3.lists.list3.description}}</p>
                </div>
                <div class="col-lg-3 col-md-6 mt-lg-4 mt-sm-5 mt-4 w3pvt-ab">
                    <div class="">
                        <img src="{{{page.section3.lists.list4.img_list}}}" alt="" class="img-fluid">
                    </div>
                    <h4 class="feed-title my-md-3 mb-3">{{page.section3.lists.list4.title}}</h4>
                    <p>{{page.section3.lists.list4.description}}</p>
                </div>
            </div>
        </div>
    </section>
    {{/page.section3.show_or_hide}}
    <!-- Offered Services -->

    <!-- video -->
    {{#page.section4.show_or_hide}}
    <section class="video py-5" id="section4">
        <div class="container">
            <div class="inner-sec-w3ls py-lg-5 py-3">
                <div class="row">
                    <div class="col-lg-6 video-right px-lg-5 px-2">
                        <h3 class="tittle-w3">{{page.section4.title}}</h3>
                        <p class="mt-4">{{page.section4.description}}</p>

                    </div>
                    <div class="col-lg-6 video-img mt-lg-0 mt-4">
                        <iframe width="560" height="360" src="{{{page.section4.video_embed_src}}}" frameborder="0" allow="accelerometer; autoplay; encrypted-media; gyroscope; picture-in-picture" allowfullscreen></iframe>
                    </div>
                </div>
            </div>
        </div>
    </section>
    {{/page.section4.show_or_hide}}
    <!-- //video -->

    <!-- projects -->
    {{#page.section5.show_or_hide}}
    <section class="portfolio py-5" id="section5">
        <div class="container py-3">
            <h3 class="heading text-capitalize mb-md-5 mb-4">{{page.section5.title}}<span></span></h3>
            <div class="row news-grids text-center">
                {{#projects}}
                    <div class="col-md-3 col-6 gal-img">
                        <a href="#{{{slug}}}"><img src="{{{image}}}" alt="portfolio image" class="img-fluid"></a>
                    </div>
                {{/projects}}
            </div>
            <!-- portfolio popups -->
            <!-- popup-->
            {{#projects}}
            <div id="{{slug}}" class="pop-overlay animate">
                <div class="popup">
                    <img src="{{{image}}}" alt="Popup Image" class="img-fluid" />
                    <p class="mt-4">{{description}}</p>
                    <a class="close" href="#section5">&times;</a>
                </div>
            </div>
            {{/projects}}
            <!-- //popup -->
            <!-- //portfolio popups -->
        </div>
    </section>
    {{/page.section5.show_or_hide}}
    <!-- //projects -->

    <!-- blogs -->
    {{#page.section6.show_or_hide}}
    <section class="video py-5" id="section6">
        <div class="container py-md-3">
            <h3 class="heading mb-5">{{page.section6.title}}</h3>
            <div class="feature-grids row">
                <div class="col-lg-4 col-sm-6 blog-single">
                    <div class="f1 icon1 p-4">
                        <a href="/post/{{{post.slug}}}"><img src="{{{post.image}}}"/></a>
                        <a href="/post/{{{post.slug}}}"><h3 class="my-3">{{post.title}}</h3></a>
                        <p>{{post.preview}}</p>
                        <p class="date">{{post.dateString}}</p>
                    </div>
                </div>
                <div class="col-lg-4 col-sm-6">
                    <div class="f1 icon1 p-4">
                        <a href="/post/{{{post.slug}}}"><img src="{{{post1.image}}}"/></a>
                        <a href="/post/{{{post.slug}}}"><h3 class="my-3">{{post1.title}}</h3></a>
                        <p>{{post1.preview}}</p>
                        <p class="date">{{post1.dateString}}</p>
                    </div>
                </div>
                <div class="col-lg-4 col-sm-6">
                    <div class="f1 icon1 p-4">
                        <a href="/post/{{{post.slug}}}"><img src="{{{post2.image}}}"/></a>
                        <a href="/post/{{{post.slug}}}"><h3 class="my-3">{{post2.title}}</h3></a>
                        <p>{{post2.preview}}</p>
                        <p class="date">{{post2.dateString}}</p>
                    </div>
                </div>
            </div>
        </div>
    </section>
    {{/page.section6.show_or_hide}}
    <!-- //blogs -->

    <!-- register -->
    {{#page.register.show_or_hide}}
    <section class="portfolio py-5" id="register">
        <div class="container">
            <div class="inner-sec-w3ls py-lg-5 py-3">
                <div class="row">
                    <div class="col-lg-6 video-right px-lg-5 px-2">
                        <h3 class="tittle-w3">{{page.register.title}}</h3>
                        <p class="mt-4">{{page.register.description}}</p>
                    </div>
                    <div class="col-lg-6 video-img mt-lg-0 mt-4">
                        <div class="login px-sm-4 mx-auto mw-100">
                            <form id="edd_register_form" action="" method="post">
                                <div class="form-group">
                                    <label>Full Name</label>
                                    <input type="text" class="form-control" id="edd-user-firstName" placeholder="" required="">
                                </div>
                                <div class="form-group">
                                    <label>Last Name</label>
                                    <input type="text" class="form-control" id="edd-user-lastName" placeholder="" required="">
                                </div>
                                <div class="form-group">
                                    <label>Email Address</label>
                                    <input type="email" class="form-control" id="edd-user-email" placeholder="" required="">
                                </div>

                                <button id="btn-register" class="btn btn-primary submit">Register</button>
                            </form>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>
    {{/page.register.show_or_hide}}
    <!-- //register -->

    {{>footer}}

    <script>
        window.__PRELOADED__ = {{{preloaded}}}
    </script>
    <!-- Vertex library for enabling site updates from https://www.vertex360.co -->
    <script type="text/javascript" src="https://cdn.turbo360-dev.com/dist/vertex.min.js"></script>

    <script src="{{{cdn}}}/dist/js/vendor.min.js"></script>
</body>
</html>